{
  "name": "lido-keys-api",
  "version": "0.1.1",
  "description": "Lido Node Operators keys service",
  "author": "Lido team",
  "private": true,
  "license": "MIT",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lidofinance/lido-keys-api.git"
  },
  "bugs": {
    "url": "https://github.com/lidofinance/lido-keys-api/issues"
  },
  "scripts": {
    "postinstall": "rimraf ./node_modules/@ethersproject/abstract-provider/src.ts/*.ts",
    "mikro-orm": "./node_modules/mikro-orm",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@lido-nestjs/consensus": "^1.3.1",
    "@lido-nestjs/constants": "^1.1.0",
    "@lido-nestjs/contracts": "^2.3.0",
    "@lido-nestjs/execution": "^1.4.0",
    "@lido-nestjs/fetch": "^1.3.0",
    "@lido-nestjs/logger": "^1.0.1",
    "@lido-nestjs/registry": "^1.4.7",
    "@lido-nestjs/validators-registry": "^1.0.2",
    "@mikro-orm/cli": "^5.5.3",
    "@mikro-orm/core": "^5.5.3",
    "@mikro-orm/knex": "^5.5.3",
    "@mikro-orm/migrations": "^5.5.3",
    "@mikro-orm/nestjs": "^5.1.2",
    "@mikro-orm/postgresql": "^5.5.3",
    "@nestjs/common": "^8.2.5",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.5",
    "@nestjs/platform-fastify": "^8.2.5",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.4",
    "@nestjs/throttler": "^2.0.0",
    "@sentry/node": "^6.16.1",
    "@willsoto/nestjs-prometheus": "^4.4.0",
    "cache-manager": "^3.6.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ethers": "^5.5.4",
    "fastify-swagger": "^4.13.1",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.5",
    "@types/cache-manager": "^3.4.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  }
}
