{
  "name": "lido-keys-api",
  "version": "3.0.0",
  "description": "Lido Node Operators keys service",
  "author": "Lido team",
  "private": true,
  "license": "MIT",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lidofinance/lido-keys-api.git"
  },
  "bugs": {
    "url": "https://github.com/lidofinance/lido-keys-api/issues"
  },
  "scripts": {
    "postinstall": "rimraf ./node_modules/@ethersproject/abstract-provider/src.ts/*.ts",
    "mikro-orm": "./node_modules/mikro-orm",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "test -f dist/main.js && node --max-old-space-size=4096 dist/main || node --max-old-space-size=4096 dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "mikro-orm schema:drop -r && mikro-orm migration:up && jest -i --config jest-e2e.json",
    "test:e2e:debug": "mikro-orm schema:drop -r && mikro-orm migration:up && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -i --config jest-e2e.json",
    "test:e2e:docker": "docker-compose -f docker-compose.e2e.yml up --build --abort-on-container-exit",
    "typechain": "typechain --target=ethers-v5 --out-dir ./src/generated ./src/staking-router-modules/contracts/abi/*.json",
    "test:alerts": "promtool test rules alerts/*.test.yml",
    "prepare:csm": "node scripts/prepare-csm-abi.js"
  },
  "dependencies": {
    "@lido-nestjs/consensus": "^1.5.0",
    "@lido-nestjs/constants": "5.3.0",
    "@lido-nestjs/contracts": "9.7.0",
    "@lido-nestjs/execution": "^1.11.0",
    "@lido-nestjs/fetch": "^1.4.0",
    "@lido-nestjs/logger": "^1.3.2",
    "@lido-nestjs/middleware": "^1.2.0",
    "@lido-nestjs/validators-registry": "^1.3.1",
    "@mikro-orm/cli": "^5.7.13",
    "@mikro-orm/core": "^5.7.13",
    "@mikro-orm/knex": "^5.7.13",
    "@mikro-orm/migrations": "^5.7.13",
    "@mikro-orm/nestjs": "^5.1.2",
    "@mikro-orm/postgresql": "^5.7.13",
    "@nestjs/common": "^10.4.16",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.16",
    "@nestjs/platform-fastify": "^10.4.16",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/terminus": "^10.3.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/cache-manager": "^2.3.0",
    "@sentry/node": "^6.16.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/static": "^7.0.4",
    "@typechain/ethers-v5": "^11.1.2",
    "@types/k6": "~0.44.2",
    "@willsoto/nestjs-prometheus": "^5.5.1",
    "cache-manager": "^5.7.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ethers": "^5.5.4",
    "jsonstream": "^1.0.3",
    "pg-query-stream": "^4.5.3",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "typechain": "^8.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.16",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.1",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
